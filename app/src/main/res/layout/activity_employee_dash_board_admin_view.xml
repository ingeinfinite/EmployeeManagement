<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:card-view="http://schemas.android.com/apk/res-auto"
    xmlns:card_view="http://schemas.android.com/tools"
    android:focusable="false"
    tools:context="app.zingo.employeemanagements.UI.NewAdminDesigns.AdminDashBoardFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:focusable="false"
        android:orientation="vertical">





            <android.support.v4.widget.NestedScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/scroll_view"
                android:focusable="false"
                android:scrollbars="none">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="150dp"
                        android:background="@color/colorPrimary"
                        android:orientation="vertical"
                        android:layout_gravity="center"
                        >

                        <de.hdodenhof.circleimageview.CircleImageView
                            android:id="@+id/employee_pic"
                            android:layout_width="100dp"
                            android:layout_height="100dp"
                            android:layout_gravity="left|top"
                            android:layout_marginLeft="10dp"
                            android:elevation="50dp"
                            android:scaleType="centerCrop"
                            android:src="@drawable/profile_image" />
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/name"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text=""
                                android:layout_weight="1"
                                android:gravity="left"
                                android:layout_marginLeft="5dp"
                                android:textColor="#fff"
                                android:textSize="22dp"
                                android:layout_marginTop="5dp"/>

                            <app.zingo.employeemanagements.Custom.CustomSpinner
                                android:id="@+id/spinner"
                                style="@style/Widget.AppCompat.Spinner"
                                android:layout_marginRight="10dp"
                                android:layout_weight="1"
                                android:layout_width="300dp"
                                android:dropDownWidth="wrap_content"
                                android:layout_height="40dp"
                                android:dropDownVerticalOffset="?attr/dropdownListPreferredItemHeight"
                                android:background="@drawable/spinner_bg"
                                android:popupBackground="@android:color/white"
                                android:stateListAnimator="@drawable/spinner_sla"
                                android:popupElevation="3dp" />

                        </LinearLayout>




                    </LinearLayout>

                    <app.zingo.employeemanagements.Custom.MyRegulerText
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="14sp"
                        android:id="@+id/atten_title"
                        android:layout_below="@+id/cardview_data_count"
                        android:gravity="left"
                        android:text="Today Summary"
                        android:paddingLeft="10dp"
                        android:paddingTop="10dp"
                        android:textColor="#000"
                        />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="16dp">

                        <LinearLayout
                            android:layout_weight="1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <TextView
                                android:id="@+id/completed_task_count_text"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="0"
                                android:textSize="25dp"
                                android:textColor="#009293"
                                android:gravity="center"
                                android:textStyle="bold"/>
                            <TextView
                                android:id="@+id/bookmark"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Tasks"
                                android:textColor="#000"
                                android:gravity="center"/>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_weight="1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <TextView
                                android:id="@+id/meeting_count_text"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="0"
                                android:textSize="25dp"
                                android:textColor="#009293"
                                android:gravity="center"
                                android:textStyle="bold"
                                />
                            <TextView
                                android:id="@+id/channel"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Meetings"
                                android:textColor="#000"
                                android:gravity="center"/>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_weight="1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <TextView
                                android:id="@+id/worked_days_count"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="0"
                                android:textSize="25dp"
                                android:textColor="#009293"
                                android:gravity="center"
                                android:textStyle="bold"/>
                            <TextView
                                android:id="@+id/comment"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Expenses"
                                android:textColor="#000"
                                android:gravity="center"/>
                        </LinearLayout>


                    </LinearLayout>

                    <android.support.v7.widget.CardView
                        android:layout_width="match_parent"
                        android:id="@+id/cardview_data_count"
                        android:visibility="gone"
                        android:layout_height="100dp"
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp"
                        card-view:cardElevation="5dp"
                        card_view:cardCornerRadius="20dp">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:id="@+id/worked_days_layout"
                                android:orientation="vertical">

                                <app.zingo.employeemanagements.Custom.MyRegulerText
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textSize="14sp"

                                    android:gravity="center_horizontal"
                                    android:text="Worked Days"
                                    android:paddingLeft="10dp"
                                    android:paddingTop="10dp"
                                    android:textColor="#000"
                                    />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:gravity="center_horizontal"
                                    android:orientation="horizontal">
                                    <app.zingo.employeemanagements.Custom.MyRegulerText

                                        android:layout_width="wrap_content"
                                        android:layout_height="match_parent"
                                        android:textSize="30sp"

                                        android:gravity="center"
                                        android:text="0"
                                        android:paddingTop="5dp"
                                        android:paddingLeft="15dp"
                                        android:textColor="#AFB42B"
                                        />


                                </LinearLayout>
                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:id="@+id/task_completed_layout"
                                android:orientation="vertical">

                                <app.zingo.employeemanagements.Custom.MyRegulerText
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textSize="14sp"
                                    android:gravity="center_horizontal"
                                    android:text="Completed Tasks"
                                    android:paddingLeft="10dp"
                                    android:paddingTop="10dp"
                                    android:textColor="#000"
                                    />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:gravity="center_horizontal"
                                    android:orientation="horizontal">
                                    <app.zingo.employeemanagements.Custom.MyRegulerText

                                        android:layout_width="wrap_content"
                                        android:layout_height="match_parent"
                                        android:gravity="center"
                                        android:textSize="30sp"
                                        android:text="0"
                                        android:paddingTop="5dp"
                                        android:paddingLeft="15dp"
                                        android:textColor="#FBC02D"
                                        />


                                </LinearLayout>
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:id="@+id/meeting_count_lay"
                                android:orientation="vertical">

                                <app.zingo.employeemanagements.Custom.MyRegulerText
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textSize="14sp"
                                    android:gravity="center_horizontal"
                                    android:text="Total Meetings"
                                    android:paddingLeft="10dp"
                                    android:paddingTop="10dp"
                                    android:textColor="#000"
                                    />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:gravity="center_horizontal"
                                    android:orientation="horizontal">
                                    <app.zingo.employeemanagements.Custom.MyRegulerText

                                        android:layout_width="wrap_content"
                                        android:layout_height="match_parent"
                                        android:gravity="center"
                                        android:textSize="30sp"
                                        android:text="0"
                                        android:paddingTop="5dp"
                                        android:paddingLeft="15dp"
                                        android:textColor="#D81B60"
                                        />


                                </LinearLayout>
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:id="@+id/expenses_lay"
                                android:orientation="vertical">

                                <app.zingo.employeemanagements.Custom.MyRegulerText
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textSize="14sp"
                                    android:gravity="center_horizontal"
                                    android:text="Total Expenses"
                                    android:paddingLeft="10dp"
                                    android:paddingTop="10dp"
                                    android:textColor="#000"
                                    />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:gravity="center_horizontal"
                                    android:orientation="horizontal">
                                    <app.zingo.employeemanagements.Custom.MyRegulerText
                                        android:id="@+id/expense_count_text"
                                        android:layout_width="wrap_content"
                                        android:layout_height="match_parent"
                                        android:gravity="center"
                                        android:textSize="30sp"

                                        android:text="0"
                                        android:paddingTop="5dp"
                                        android:paddingLeft="15dp"
                                        android:textColor="#F44336"
                                        />


                                </LinearLayout>
                            </LinearLayout>
                        </LinearLayout>
                    </android.support.v7.widget.CardView>



                    <LinearLayout

