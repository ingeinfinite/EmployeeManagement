                                android:layout_height="wrap_content"
                                android:text=""
                                android:id="@+id/logged_text"
                                android:textColor="@color/schedulecolor"
                                android:letterSpacing="0.025"
                                android:layout_marginLeft="30dp"
                                android:textSize="18dp"

                                />


                            <app.zingo.employeemanagements.Custom.MyRegulerText
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text=""
                                android:id="@+id/check_in_address"
                                android:textColor="@color/statementcolor"
                                android:letterSpacing="0.025"
                                android:layout_marginLeft="30dp"
                                android:textSize="12dp"
                                android:layout_marginTop="5dp"/>
                        </LinearLayout>



                    </LinearLayout>


                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.3dp"
                        android:background="#99A1AC"
                        android:layout_marginTop="2dp"/>

                    <LinearLayout

                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="10dp">


                        <ImageView
                            android:layout_width="22dp"
                            android:layout_height="25dp"
                            android:layout_gravity="center"
                            android:layout_marginLeft="50dp"
                            android:background="@drawable/logout_time" />


                        <app.zingo.employeemanagements.Custom.MyRegulerText
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="18dp"
                            android:id="@+id/logout_time"
                            android:textColor="@color/text_black"
                            android:letterSpacing="0.025"
                            android:layout_marginLeft="25dp"
                            android:layout_marginTop="10dp"/>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <app.zingo.employeemanagements.Custom.MyRegulerText
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text=""
                                android:id="@+id/log_out__text"
                                android:textColor="@color/schedulecolor"
                                android:letterSpacing="0.025"
                                android:layout_marginLeft="30dp"
                                android:textSize="18dp"

                                />


                            <app.zingo.employeemanagements.Custom.MyRegulerText
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text=""
                                android:id="@+id/check_out_address"
                                android:textColor="@color/statementcolor"
                                android:letterSpacing="0.025"
                                android:layout_marginLeft="30dp"
                                android:textSize="12dp"
                                android:layout_marginTop="5dp"/>
                        </LinearLayout>



                    </LinearLayout>


                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.3dp"
                        android:background="#99A1AC"
                        android:layout_marginTop="2dp"/>

                    <LinearLayout

                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="10dp">


                        <ImageView
                            android:layout_width="22dp"
                            android:layout_height="25dp"
                            android:layout_gravity="center"
                            android:layout_marginLeft="50dp"
                            android:background="@drawable/meeting_time" />


                        <app.zingo.employeemanagements.Custom.MyRegulerText
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="18dp"
                            android:id="@+id/meeting_time"
                            android:textColor="@color/text_black"
                            android:letterSpacing="0.025"
                            android:layout_marginLeft="25dp"
                            android:layout_marginTop="10dp"/>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <app.zingo.employeemanagements.Custom.MyRegulerText
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text=""
                                android:id="@+id/meeting__text"
                                android:textColor="@color/schedulecolor"
                                android:letterSpacing="0.025"
                                android:layout_marginLeft="30dp"
                                android:textSize="18dp"

                                />



                        </LinearLayout>



                    </LinearLayout>


                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.3dp"
                        android:background="#99A1AC"
                        android:layout_marginTop="2dp"/>

                    <LinearLayout

                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="10dp">


                        <ImageView
                            android:layout_width="22dp"
                            android:layout_height="25dp"
                            android:layout_gravity="center"
                            android:layout_marginLeft="50dp"
                            android:background="@drawable/expense_admin" />


                        <app.zingo.employeemanagements.Custom.MyRegulerText
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="18dp"
                            android:id="@+id/expenses_text"
                            android:textColor="@color/text_black"
                            android:letterSpacing="0.025"
                            android:layout_marginLeft="25dp"
                            android:layout_marginTop="10dp"/>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <app.zingo.employeemanagements.Custom.MyRegulerText
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text=""
                                android:id="@+id/expense_amount"
                                android:textColor="@color/schedulecolor"
                                android:letterSpacing="0.025"
                                android:layout_marginLeft="30dp"
                                android:textSize="18dp"

                                />



                        </LinearLayout>



                    </LinearLayout>


                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.3dp"
                        android:background="#99A1AC"
                        android:layout_marginTop="2dp"/>

                    <app.zingo.employeemanagements.Custom.MyRegulerText
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="14sp"

                        android:layout_below="@+id/cardview_data_count"
                        android:gravity="left"
                        android:text="Today Tasks"
                        android:paddingLeft="10dp"
                        android:paddingTop="10dp"
                        android:textColor="#000"
                        />

                    <LinearLayout
                        android:gravity="center"
                        android:orientation="vertical"
                        android:id="@+id/noRecordFound"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="40dp"

                        android:layout_below="@+id/linearLayout2">
                        <ImageView
                            android:id="@+id/imageView2"
                            android:background="@color/white"
                            android:layout_width="48dp"
                            android:layout_height="48dp"
                            android:src="@drawable/search"
                            android:minHeight="100dp"
                            android:alpha="0.7"/>
                        <TextView
                            android:textSize="15sp"
                            android:textColor="@color/text_grey"
                            android:gravity="center_horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="No Task Found"/>
                    </LinearLayout>
                    <android.support.v7.widget.RecyclerView
                        android:id="@+id/targetList"
                        android:visibility="gone"
                        android:focusable="false"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"/>

                    <app.zingo.employeemanagements.Custom.MyRegulerText
                        android:id="@+id/read_task"
                        android:layout_weight="1"
                        android:layout_width="150dp"
                        android:visibility="gone"
                        android:layout_height="45dp"
                        android:background="@drawable/button_primary_dark"
                        android:layout_margin="5dp"
                        android:textAlignment="center"
                        android:gravity="center"
                        android:focusable="false"
                        android:layout_gravity="right"
                        android:padding="5dp"
                        android:text="Read More"
                        android:textSize="22dp"
                        android:textColor="@color/white"
                        />


                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.3dp"
                        android:background="#99A1AC"
                        android:layout_marginTop="2dp"/>

                    <app.zingo.employeemanagements.Custom.MyRegulerText
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="14sp"

                        android:layout_below="@+id/cardview_data_count"
                        android:gravity="left"
                        android:text="Today Meetings"
                        android:paddingLeft="10dp"
                        android:paddingTop="10dp"
                        android:textColor="#000"
                        />

                    <LinearLayout
                        android:gravity="center"
                        android:orientation="vertical"
                        android:id="@+id/noRecordFound_meetings"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="40dp"

                        android:layout_below="@+id/linearLayout2">
                        <ImageView
                            android:id="@+id/imageView2_meeting"
                            android:background="@color/white"
                            android:layout_width="48dp"
                            android:layout_height="48dp"
                            android:src="@drawable/search"
                            android:minHeight="100dp"
                            android:alpha="0.7"/>
                        <TextView
                            android:textSize="15sp"
                            android:textColor="@color/text_grey"
                            android:gravity="center_horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="No Meetings Found"/>
                    </LinearLayout>
                    <android.support.v7.widget.RecyclerView
                        android:id="@+id/targetList_meeting"
                        android:visibility="gone"
                        android:focusable="false"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"/>

                    <app.zingo.employeemanagements.Custom.MyRegulerText
                        android:id="@+id/read_meeting"
                        android:layout_weight="1"
                        android:layout_width="150dp"
                        android:visibility="gone"
                        android:layout_height="45dp"
                        android:background="@drawable/button_primary_dark"
                        android:layout_margin="5dp"
                        android:textAlignment="center"
                        android:gravity="center"
                        android:focusable="false"
                        android:layout_gravity="right"
                        android:padding="5dp"
                        android:text="Read More"
                        android:textSize="22dp"
                        android:textColor="@color/white"
                        />





